{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "sort-exports"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "sort-exports/sort-exports": [
      "error",
      {
        "sortDir": "asc"
      }
    ]
  }
}